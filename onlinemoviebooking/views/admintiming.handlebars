<div class="main-body">
     <div class="add-form-section">
         <form action="/timing/add" method="post">
            <div class="form-group">
             <input type="text" name="timing" placeholder="Name" id="" class="form-control">
            </div>
            <div class="form-group">
                <button type="submit" class="btn-primary">Add</button>
            </div>
         </form>
     </div>
     <div class="update-form-section">
         <form  id="update-timing-form">
             <div class="form-group">
                 <input type="number" name="" id="id" class="form-control" readonly>
             </div>
             <div class="form-group">
                 <input type="text" name="timing" id="name" class="form-control">
             </div>
            <div class="form-group">
                <button class="btn-primary">Update</button>
            </div>
         </form>
     </div>
</div>
<div class="table-section">
    <table>
        <thead>
            <tr>
                <th>Id</th>
                <th>timing</th>
                <th colspan="2">Action</th>
            </tr>
        </thead>
        <tbody>
            {{#each timings}}
                <tr>
                    <td>{{this.id}}</td>
                    <td>{{this.timing}}</td>
                    <td><button class="btn-alert">Edit</button></td>
                    <td><button class="btn-danger">Delete</button></td>
                </tr>
            {{/each}}
        </tbody>
    </table>
</div>